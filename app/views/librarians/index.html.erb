  <div class="panel panel-success">
    <div class="panel-heading"><h1>Library Management</h1></div>
    <div class="panel-body">
    <h3>Find users</h3>
    <table class="table">
      <tr>
       <%= form_tag librarians_index_path, :method => 'get' do%>
         <td>IC :<%= text_field_tag :searchStu, params[:searchStu]%></td>
         <td colspan="7"><%= submit_tag "Search"%></td>
         
       <% end %>
       </tr>
      <tr>
        <th>Title</th>
        <th>Text</th>
        <th colspan="3"></th>
      </tr>
     

      <% @users.each do |user| %>
        <tr>
          <td><%= user.user_ic %></td>
          <td><%= user.user_name %></td>
          <td><%= user.user_email %></td>
          <td><%= user.user_level %></td>
          <td><%= user.user_address %></td>
          <td><%= user.user_contact %></td>

          <td><%= link_to 'View', librarian_path(user), class: "btn btn-warning" %></td>

        </tr>
      <% end %>
    </table>
    </div>
  </div>
